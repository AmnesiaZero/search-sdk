{
  "name": "search/sdk",
  "description": "Sdk to send request for search api",
  "type": "sdk",
  "autoload": {
    "psr-4": {
      "Search\\Sdk\\": "src/",
      "Search\\Sdk\\Core\\" : "src/core",
      "Search\\Sdk\\Logs\\" : "src/logs",
      "Search\\Sdk\\Models\\": "src/models",
      "Search\\Sdk\\Test\\" : "src/test"
    }
  },
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "firebase/php-jwt": "^6.9",
    "monolog/monolog": "^3.5",
    "ext-http": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
